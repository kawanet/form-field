{
  "name": "html-form-field",
  "description": "Unified interface for HTML form fields with synchronized binding to object properties",
  "version": "0.2.0",
  "author": "Kawanet",
  "c8": {
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "reportsDir": "coverage"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-multi-entry": "^7.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-sucrase": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.9.1",
    "c8": "^10.1.3",
    "chai": "^6.2.0",
    "html-ele": "^0.0.1",
    "jsdom": "^27.0.1",
    "mocha": "^11.7.4",
    "rollup": "^4.52.5",
    "terser": "^5.44.0",
    "typescript": "^5.9.3"
  },
  "exports": {
    ".": {
      "require": "./dist/html-form-field.cjs",
      "import": {
        "types": "./types/html-form-field.d.ts",
        "default": "./dist/html-form-field.mjs"
      }
    }
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist/html-form-field.cjs",
    "dist/html-form-field.min.js",
    "dist/html-form-field.mjs",
    "dist/package.json",
    "src/*.ts",
    "types/*.d.ts"
  ],
  "license": "MIT",
  "main": "./src/index.ts",
  "scripts": {
    "build": "make -C builder",
    "fixpack": "fixpack",
    "prepare": "make -C builder clean all test",
    "test": "node --test",
    "test:coverage": "c8 node --test"
  },
  "sideEffects": false,
  "type": "module",
  "types": "types/html-form-field.d.ts"
}
